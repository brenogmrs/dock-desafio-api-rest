    "date-fns-tz": "^1.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "pg": "^8.7.1",
    "pre-push": "^0.1.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.6",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.41",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
